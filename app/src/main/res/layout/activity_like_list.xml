<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    app:layout_editor_absoluteX="1dp"
    app:layout_editor_absoluteY="1dp"
    android:fillViewport="true"
    android:visibility="visible"
    tools:ignore="ScrollViewCount">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".ProfileSwapActivity">

        <ImageButton
            android:id="@+id/backbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="30dp"

            android:background="@null"
            android:src="@drawable/ic_backbutton"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="SpeakableTextPresentCheck"></ImageButton>

        <TextView
            android:id="@+id/likelisttxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:text="Your likes list"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold"


            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/alredyliked"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="100dp"
            android:layout_marginEnd="30dp"
            android:text="These people have alredy liked you - like them \n back and it`s a match "
            android:textAlignment="center"
            android:textColor="#000000"


            android:textSize="16sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/user1"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="170dp"
            android:src="@drawable/ic_user1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/trueLike"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:background="@null"
            android:src="@drawable/ic_true"
            app:layout_constraintBottom_toBottomOf="@+id/falseLike"
            app:layout_constraintEnd_toEndOf="@+id/user1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/falseLike"
            app:layout_constraintTop_toBottomOf="@+id/user1"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/falseLike"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_false"
            app:layout_constraintStart_toStartOf="@+id/user1"
            app:layout_constraintTop_toBottomOf="@+id/user1"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/user2"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="170dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_user2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/user1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <ImageButton
            android:id="@+id/trueLike2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_true"
            app:layout_constraintEnd_toEndOf="@+id/user2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/falseLike2"
            app:layout_constraintTop_toBottomOf="@+id/user2"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/falseLike2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_false"
            app:layout_constraintStart_toStartOf="@+id/user2"
            app:layout_constraintTop_toBottomOf="@+id/user2"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/user3"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/ic_user3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/falseLike"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/trueLike3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_true"
            app:layout_constraintBottom_toBottomOf="@+id/falseLike3"
            app:layout_constraintEnd_toEndOf="@+id/user3"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/falseLike3"
            app:layout_constraintTop_toBottomOf="@+id/user3"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/falseLike3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_false"
            app:layout_constraintEnd_toStartOf="@+id/trueLike3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/user3"
            app:layout_constraintTop_toBottomOf="@+id/user3"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/user4"
            android:layout_width="165dp"
            android:layout_height="165dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/ic_user4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/falseLike2"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/trueLike4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_true"
            app:layout_constraintEnd_toEndOf="@+id/user4"
            app:layout_constraintStart_toEndOf="@+id/falseLike4"
            app:layout_constraintTop_toBottomOf="@+id/user4"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/falseLike4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_false"
            app:layout_constraintBottom_toBottomOf="@+id/trueLike4"
            app:layout_constraintEnd_toStartOf="@+id/trueLike4"
            app:layout_constraintStart_toStartOf="@+id/user4"
            app:layout_constraintTop_toBottomOf="@+id/user4"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/ic_user5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/trueLike3"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/trueLike5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_true"
            app:layout_constraintEnd_toEndOf="@+id/imageView4"
            app:layout_constraintStart_toEndOf="@+id/falseLike5"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/falseLike5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="11dp"
            android:layout_marginBottom="22dp"
            android:background="@null"
            android:src="@drawable/ic_false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/trueLike5"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintVertical_bias="0.53999996"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>